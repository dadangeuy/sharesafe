<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<HBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121">
    <ListView fx:id="filesView" prefHeight="200.0" prefWidth="200.0"/>
    <VBox prefHeight="200.0" prefWidth="100.0">
        <Button fx:id="uploadBtn" mnemonicParsing="false" text="Upload"/>
        <Button fx:id="downloadBtn" mnemonicParsing="false" text="Download"/>
    </VBox>
</HBox>
