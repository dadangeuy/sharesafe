<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0" spacing="10.0"
      xmlns="http://javafx.com/javafx/8.0.121">
    <HBox spacing="10.0">
        <ListView fx:id="filesView" HBox.hgrow="ALWAYS"/>
        <VBox spacing="10.0">
            <Button fx:id="connectBtn" mnemonicParsing="false" prefWidth="100.0" text="Connect"/>
            <Button fx:id="uploadBtn" mnemonicParsing="false" prefWidth="100.0" text="Upload"/>
            <Button fx:id="downloadBtn" mnemonicParsing="false" prefWidth="100.0" text="Download"/>
        </VBox>
    </HBox>
    <Label fx:id="errorView" text="Error" textFill="RED" visible="false"/>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</VBox>
